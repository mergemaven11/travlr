<!DOCTYPE html>
<html>
<body>
	<div id="contents">
		<div class="box">
			<div>
				<div class="body">
					<h1>Travel</h1>
					<ul id="sites">
						{{#each trips}}
						<li>
							<a href="/api/trips/{{this.code}}"><img src="images/{{this.image}}" alt="Img"></a>
							<h2><a href="/travel">{{this.name}}</a></h2>
							{{{this.description}}}
						</li>
						{{/each}}
					</ul>
				</div>
			</div>
		</div>
	</div>
</body>
</html>
